{
  "name": "speedy",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "build": "webpack",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,graphql,gql}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@vtex/prettier-config": "^0.1.3",
    "babel-loader": "^9.1.3",
    "eslint": "^6.8.0",
    "eslint-config-vtex": "^12.2.1",
    "eslint-config-vtex-react": "^6.2.1",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^4.2.0",
    "inline-chunk-html-plugin": "^1.1.1",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "typescript": "^3.7.5",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
